Windows Registry Editor Version 5.00

;rmdir

[HKEY_CLASSES_ROOT\Directory\shell\rmdir]
@="Remove Directory"
"NoWorkingDirectory"=""
"Icon"="rcwmimg.dll,-2"

[HKEY_CLASSES_ROOT\Directory\shell\rmdir\command]
@="cmd /c del /s /q \"%1\"\\* && rd /s /q \"%1\""
"IsolatedCommand"="cmd /c del /s /q \"%1\"\\* && rd /s /q \"%1\""

;done
